{
  "name": "Strong Password Generator",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Generate strong password for the most secret.",
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon24.png",
      "38": "images/icon48.png"
    },
    "default_title": "Strong Password Generator",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "http://*/",
    "https://*/"
  ]
}